-- Function to copy text to the clipboard
local function CopyToClipboard(text)
    if setclipboard then
        setclipboard(text)
    elseif toclipboard then
        toclipboard(text)
    else
        warn(YLXibivMLUPSLGYtxozxOxQDXcBmjHEIyoLTTKnBUJLasrTCNqNUxogkRhBynxBZmPAYUVoPTYuMfsqynkggoDEvMO('jZsasCsMcEzcAOweJwciCoLlLtCGIDyZiPwRXgZvYgccYeNcNOtlYYamzLxjTIbCMzSwqQMVkEHgDlvyFtUqtXaXBrzyKqbiORpycCYFplRPqiHlIXDHKLhNKTsPBCrnsXQfdsLQ2xpcGJvYXJkIGZ1bmN0aW9uYWxpdHkgbm90IHN1cHBvcnRlZCBvbiB0aGlzIGV4ZWN1dG9yLg=='))
    end
end

